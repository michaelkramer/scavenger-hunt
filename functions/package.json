{
  "name": "functions",
  "scripts": {
    "clean": "rm -r build",
    "lint": "eslint --ext .ts,.tsx .",
    "build-no": "tsc -w --preserveWatchOutput",
    "serve": "firebase emulators:start --only functions,hosting",
    "export": "firebase emulators:export ../data",
    "shell": "yarn build && firebase functions:shell",
    "start": "concurrently -c 'bgBlue.bold,bgMagenta.bold' -n 'code,server' -s 'first' 'yarn build-babel' 'yarn serve'",
    "start3": "concurrently -c 'bgBlue.bold,bgGreen.bold,bgMagenta.bold' -n 'code,webpack,server' -s 'first' 'yarn build:js' 'yarn buildServer' 'yarn serve'",
    "start3pack": "concurrently -c 'bgBlue.bold,bgGreen.bold,bgMagenta.bold' -n 'code,webpack,server' -s 'first' 'yarn build-babel' 'yarn buildServer' 'yarn serve'",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build-babel": "yarn build:types && yarn build:js",
    "build:types": "tsc --noEmit",
    "build:react": "tsc views/**/* --outDir ../public/views/ -w --preserveWatchOutput",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline --watch",
    "createfirebaseconf": "firebase setup:web --json > ../functions/firebase-config.json",
    "buildBackend": "./node_modules/.bin/webpack --mode production --config ./webpack.backend.config.js",
    "buildClient": "./node_modules/.bin/webpack --mode production --config ./webpack.client.config.js",
    "buildServer": "./node_modules/.bin/webpack --mode production --config ./webpack.server.config.js",
    "build": "concurrently -c 'bgBlue.bold,bgMagenta.bold' 'yarn buildServer' 'yarn build:js'"
  },
  "engines": {
    "node": "8"
  },
  "main": "build/index.js",
  "dependencies": {
    "antd": "^4.1.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^7.14.2",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.0",
    "firebaseui": "^4.5.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-with-firebase-auth": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/glob": "^7.1.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "firebase-functions-test": "^0.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-emit-all-plugin": "^2.0.1"
  },
  "private": true
}
